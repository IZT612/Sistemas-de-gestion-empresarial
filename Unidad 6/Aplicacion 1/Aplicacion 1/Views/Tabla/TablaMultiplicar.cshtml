@{
    var tabla = ViewBag.Tabla as string[][];
}

<table style="border-collapse: collapse; margin: 0 auto; border: 1px solid #ccc;">
    @for (int i = 0; i < tabla.Length; i++)
    {
        <tr>
            @for (int j = 0; j < tabla[i].Length; j++)
            {
                <td style="border: 1px solid #ccc; padding: 5px">
                    @tabla[i][j]
                </td>
            }
        </tr>
    }
</table>
</br>
<p> Volver al @Html.ActionLink("Ejercicio 1", "index", "Home") </p>
